{% extends "rwadmin/base.htm" %}
{%block head %}
{%block title %}{%endblock%}
{%endblock %}
{%block content%}


<div align ="center" style="background-color:#ccccc0;border:0px solid black;width:100%;height:4%; font-size:12px;color:white"> <strong>CONFIRM ACCOUNT ROLLOVER  </strong> </div>
<div align="center" style="color:red;font-family:verdana;font-size:13px;text-decoration:blink;" > {{varerr}}</div>
    <form action="/SchApp/account/controllers/confirmaccrollover/" method="post" name ="main" onsubmit="disable(this)">
<table align ="left" style="border:0px solid black;width:100%">

<tr>
        <td align ="left" style="width:auto">
<strong>{{varuser|upper}}</strong>, Are You sure you really want to <strong>ROLLOVER </strong> Accounting Year Ended :: {{endate|date:"d-m-Y"}} ?
<input name="submit" type= "submit" value="Yes ROLLOVER "><strong><a href="/SchApp/account/controllers/accrollover/" style ="color:black;font-size:14px"> NO,BACK </a></strong></strong>

                        </td>
</tr>
</table>
    </form>
    <script language="javascript" type="text/javascript">
        function disable(f)
        {
            var button = f.elements['submit'];
            button.value = 'Processing';
            button.disabled = 'true';
            return true;
        }
    </script>

 {%endblock%}
