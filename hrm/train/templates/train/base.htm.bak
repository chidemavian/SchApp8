<script language="javascript" type="text/javascript">
    function disable(f)
    {
        var button = f.elements['submit'];
        button.value = 'Processing';
        button.disabled = 'true';
        return true;
    }
</script>

<script type=text/javascript>
    $(document).ready(function() {
        openrecmainhrm();
    });
</script>
<script language="javascript" type="text/javascript">
    function openrecmainhrm()
    {
        var data='userid='+'test';
        $.ajax({
            type:"POST",
            url:"/header/",
            data:data,
            success:function(html) {
                $(".boxnn").show();
                $(".boxnn").html(html);
            }
        });
        return false;
    }
</script>

</head>

<body>

    <div id="wrapper"> 

        <div id="header"> 
            <div id="app_name">SchApp 7.0 - Enterprise School Solution</div>
            <div class="boxnn">
            </div>
        </div>

        <div id="content" >

            <div id="content_left_other">
                {%block content%}
                {%endblock%}
            </div>

            <div id="content_right_other_student">
                <div class="side_menu_header">Menu Items</div>
                <ul class="side_menu">
                    <li><a href="/hrm/train/trainingsetup/">Setup Training</a></li>
                    <li><a href="/hrm/train/trainingstaff/">Staff On Training</a></li>
                    <li><a href="/hrm/train/trainingreport/">Training Report</a></li>
                    <li><a href="/hrm/train/yettobetrained/">Yet To be Trained</a></li>
                </ul>
            </div>

        </div>

        <div id="footer"></div>

    </div>


</body>

</html>
